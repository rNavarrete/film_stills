<% if current_user %>
  <h1><%= current_user.first_name %></h1>
<% else %>
  <%= form_tag("/sessions", method: "post") do %>
    <%= label_tag(:email, "Email:") %>
    <%= text_field_tag(:email) %>
    <%= label_tag(:password, "Password:") %>
    <%= text_field_tag(:password) %>
    <%= submit_tag("Sign In") %>
  <% end %>
<% end %>




